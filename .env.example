# RBI NBFC Chatbot Environment Variables

# Gemini API Key (Get from https://ai.google.dev/gemini-api/docs/api-key)
GOOGLE_API_KEY=your_gemini_api_key_here

# LangSmith API Key (Get from https://smith.langchain.com/)
LANGSMITH_API_KEY=your_langsmith_api_key_here

# LangSmith Project Name (Optional)
LANGSMITH_PROJECT=rbi-nbfc-chatbot

# Gemini Model Configuration
GEMINI_MODEL=gemini-1.5-flash
GEMINI_EMBEDDING_MODEL=models/text-embedding-004

# Vector Store Configuration
FAISS_INDEX_PATH=data/index.faiss
CHUNK_SIZE=1200
CHUNK_OVERLAP=200
RETRIEVAL_K=4

# API Configuration
API_HOST=127.0.0.1
API_PORT=8000